mkdir -p $PWD/ArchPac/src
mkdir $PWD/ArchPac/var
mkdir $PWD/ArchPac/png
mkdir $PWD/ArchPac/log
ln -s $PWD/ArchPac/var $PWD/ArchPac/src/2var
ln -s $PWD/ArchPac/png $PWD/ArchPac/src/2png
curl -o $PWD/ArchPac/src/32_bash.sh https://raw.githubusercontent.com/SoKT/ArchPac/master/src/32_bash.sh
curl -o $PWD/ArchPac/src/pac.cpp https://raw.githubusercontent.com/SoKT/ArchPac/master/src/pac.cpp
curl -o $PWD/ArchPac/src/plot.sh https://raw.githubusercontent.com/SoKT/ArchPac/master/src/plot.sh
g++ -o $PWD/ArchPac/src/pac $PWD/ArchPac/src/pac.cpp 
. $PWD/ArchPac/src/32_bash.sh
touch $PWD/ArchPac/var/BitMaple.dat
ln -s $PWD/ArchPac/BitMaple.dat $PWD/ArchPac/src/2BitMaple.dat
ln -s $PWD/ArchPac/Packages.dat $PWD/ArchPac/src/2Packages.dat
ln -s $PWD/ArchPac/Required.dat $PWD/ArchPac/src/2Required.dat
$PWD/ArchPac/src/pac 1>$PWD/ArchPac/log/PacOutput.log 2>$PWD/ArchPac/log/PacError.log
. $PWD/ArchPac/src/plot.sh 1>$PWD/ArchPac/log/PlotOutput.log  2>$PWD/ArchPac/log/PlotError.log